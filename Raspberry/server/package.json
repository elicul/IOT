{
   "name": "server",
   "version": "0.0.0",
   "description": "description",
   "author": "Enzo",
   "license": "MIT",
   "scripts": {
      "format": "prettier --write \"**/*.ts\"",
      "start": "ts-node src/main",
      "prestart:prod": "tsc",
      "start:prod": "node dist/main.js",
      "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -n Init",
      "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run",
      "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert",
      "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm schema:sync",
      "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm schema:drop"
   },
   "dependencies": {
      "@nestjs/common": "^5.0.0",
      "@nestjs/core": "^9.2.1",
      "@nestjs/passport": "^1.1.0",
      "@nestjs/swagger": "^6.1.3",
      "@nestjs/typeorm": "^5.1.0",
      "class-validator": "^0.9.1",
      "fastify-formbody": "^2.0.0",
      "jsonwebtoken": "^8.3.0",
      "lodash": "^4.17.11",
      "moment": "^2.22.2",
      "mysql": "^2.14.1",
      "passport": "^0.4.0",
      "passport-http-bearer": "^1.0.1",
      "passport-jwt": "^4.0.0",
      "reflect-metadata": "^0.1.10",
      "rxjs": "^6.0.0",
      "typeorm": "0.2.7",
      "typescript": "^2.6.2"
   },
   "devDependencies": {
      "@nestjs/testing": "^5.0.0",
      "@types/express": "^4.0.39",
      "@types/jest": "^21.1.8",
      "@types/lodash": "^4.14.117",
      "@types/moment": "^2.13.0",
      "@types/node": "^8.0.29",
      "@types/supertest": "^2.0.4",
      "jest": "^21.2.1",
      "nodemon": "^1.14.1",
      "prettier": "^1.11.1",
      "supertest": "^3.0.0",
      "ts-jest": "^21.2.4",
      "ts-loader": "^4.1.0",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.1.1",
      "tslint": "5.3.2",
      "webpack": "^4.2.0",
      "webpack-cli": "^2.0.13",
      "webpack-node-externals": "^1.6.0",
      "typescript": "2.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "../coverage"
   }
}
